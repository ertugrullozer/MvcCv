@model MvcCv.Models.Entity.TBLsertifikalar
@{
    ViewBag.Title = "YeniSertifika";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h4>Yeni Sertifika Ekleme Sayfası</h4>

<form method="post">

    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })

    <br />
    @Html.Label("Sertifika Açıklaması")
    @Html.TextBoxFor(x => x.Sertifikalar, new { @class = "form-control" })
    <br />


    <br />
    <button class="btn btn-success">Kaydet</button>
</form>
