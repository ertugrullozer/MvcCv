@model MvcCv.Models.Entity.TBLyetenekler
@{
    ViewBag.Title = "Düzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4>Yeni Yetenek Düzenleme Sayfası</h4>
<form method="post">

    @Html.Label("Yetenek Açıklaması")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    <br />
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />

    <button class="btn btn-info mt-2">Kaydet</button>

</form>