@model MvcCv.Models.Entity.TBLsosyalmedya

@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h4>Sosyal Medya Düzenleme Sayfası</h4>
<form method="post">

    @Html.Label("Sosyal Medya İsmi")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Sosyal Medya Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Sosyal Medya İkon")
    @Html.TextBoxFor(x => x.ikon, new { @class = "form-control" })
    <br />

    <button class="btn btn-info mt-2">Kaydet</button>

</form>